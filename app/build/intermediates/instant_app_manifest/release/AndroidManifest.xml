<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.mstudyplanner"
    android:targetSandboxVersion="2"
    android:versionCode="3"
    android:versionName="3.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:icon="@drawable/ic_mstudy"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_mstudy"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true" >
        <activity android:name="com.gmail.mstudyplanner.RegisterActivity" />
        <activity android:name="com.gmail.mstudyplanner.LoginActivity" />
        <activity android:name="com.gmail.mstudyplanner.MyPageActivity" />
        <activity android:name="com.gmail.mstudyplanner.Main_calendar" />
        <activity android:name="com.gmail.mstudyplanner.Main_study" />
        <activity android:name="com.gmail.mstudyplanner.Subject_input" />
        <activity android:name="com.gmail.mstudyplanner.Main_subject" />
        <activity android:name="com.gmail.mstudyplanner.Wisdom_input" />
        <activity android:name="com.gmail.mstudyplanner.Dday_list" />
        <activity android:name="com.gmail.mstudyplanner.DdayInputActivity" />
        <activity android:name="com.gmail.mstudyplanner.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>